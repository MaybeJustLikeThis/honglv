{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
    
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential"
    // 告诉 ESLint 关闭与 Prettier 格式化规则冲突的任何规则，需写在最后，会覆盖前面的配置
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {}
}
